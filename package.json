{
  "name": "garcia-starter-kit",
  "version": "0.0.1",
  "description": "Esqueleto de API pronto para iniciar novos projetos rapidamente.",
  "author": "Mateus Garcia",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "open": "nohup google-chrome http://localhost:3000/api > /dev/null 2>&1 &",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "infra": "docker-compose -f docker-compose.yml up -d",
    "stop:all": "docker stop $(docker ps -q -a) && docker rm $(docker ps -q -a)"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.11",
    "@nestjs/core": "^8.0.11",
    "@nestjs/platform-express": "^8.0.11",
    "@nestjs/swagger": "^5.1.0",
    "@nestjs/typeorm": "^8.0.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "dotenv": "^10.0.0",
    "pg": "^8.7.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "socket.io": "^4.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.38",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "^8.0.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.10.3",
    "@types/redis": "^2.8.32",
    "@types/uuid": "^8.3.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
}